{
  "deno.enable": true,
  "deno.lint": true,
  "files.exclude": {
    "**/_build": true,
    "**/.elixir_ls": true,
    "**/deps": true,
    "**/cover": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true
  },
  "[elixir]": {
    "editor.defaultFormatter": "JakeBecker.elixir-ls"
  },
  "[deno]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
  }
}
